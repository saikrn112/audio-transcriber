# HuggingFace token for speaker diarization
# Get your token from https://huggingface.co/settings/tokens
HUGGINGFACE_TOKEN=your_token_here

# GPU Configuration
# Set to 0 to disable GPU, 1 to enable
USE_GPU=1

# Flask Configuration
FLASK_PORT=7000
FLASK_DEBUG=0

# Model Configuration
# Maximum number of speakers to detect in audio files
DEFAULT_MAX_SPEAKERS=2
